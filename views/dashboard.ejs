<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />

    <title>iPortfolio Bootstrap Template - Index</title>
    <meta content="" name="description" />
    <meta content="" name="keywords" />

    <!-- Favicons -->
    <link href="../dash/assets/img/favicon.png" rel="icon" />
    <link
      href="../dash/assets/img/apple-touch-icon.png"
      rel="apple-touch-icon"
    />

    <!-- Google Fonts -->
    <link
      href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i"
      rel="stylesheet"
    />

    <!-- Vendor CSS Files -->
    <link href="../dash/assets/vendor/aos/aos.css" rel="stylesheet" />
    <link
      href="../dash/assets/vendor/bootstrap/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link
      href="../dash/assets/vendor/bootstrap-icons/bootstrap-icons.css"
      rel="stylesheet"
    />
    <link
      href="../dash/assets/vendor/boxicons/css/boxicons.min.css"
      rel="stylesheet"
    />
    <link
      href="../dash/assets/vendor/glightbox/css/glightbox.min.css"
      rel="stylesheet"
    />
    <link
      href="../dash/assets/vendor/swiper/swiper-bundle.min.css"
      rel="stylesheet"
    />

    <!-- Template Main CSS File -->
    <link href="../dash/assets/css/style.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="../stylesheets/user.css" />
  </head>

  <body>
    <!-- ======= Mobile nav toggle button ======= -->
    <i class="bi bi-list mobile-nav-toggle d-xl-none"></i>

    <!-- ======= Header ======= -->
    <header id="header">
      <div class="d-flex flex-column">
        <div class="profile">
          <img
            src="../dash/assets/img/default.png"
            alt=""
            class="img-fluid rounded-circle"
          />
          <h1 class="text-light"><a href="index.html"> <%= fullname %> </a></h1>
          <div class="social-links mt-3 text-center">
            <a href="#" class="twitter"><i class="bx bxl-twitter"></i></a>
            <a href="#" class="facebook"><i class="bx bxl-facebook"></i></a>
            <a href="#" class="instagram"><i class="bx bxl-instagram"></i></a>
            <a href="#" class="github"><i class="bx bxl-github"></i></a>
            <a href="#" class="linkedin"><i class="bx bxl-linkedin"></i></a>
          </div>
        </div>

        <nav id="navbar" class="nav-menu navbar">
          <ul>
            <li>
              <a href="#hero" class="nav-link scrollto active"
                ><i class="bx bx-home"></i> <span>Home</span></a>
            </li>
           
            <li>
              <a href="#about" class="nav-link scrollto"
                ><i class="bx bx-user"></i> <span>About</span></a>
            </li>

            <li>
              <a href="#resume" class="nav-link scrollto"
                ><i class="bi bi-person-fill-up"></i> <span>Hire An Agent </span></a>
            </li>

            <li>
              <a href="#services"
                class="nav-link scrollto"
                onclick="fetchServiceHistory()">
                <i class="bx bx-server"></i> <span>Services</span></a>
            </li>

              <li>
              <a href="#feedback" class="nav-link scrollto"
                ><i class="bi bi-chat-square-text"></i> <span>Feedback and Rating</span></a>
            </li>

            <li>
              <a href="#history" class="nav-link scrollto"
                ><i class="bi bi-clock-history"></i> <span>History</span></a>
            </li>

            <li>
              <a href="#contact" class="nav-link scrollto"
                ><i class="bx bx-envelope"></i> <span>Contact</span></a>
            </li>

            <li>
              <a
                href="/users/logout"
                class="nav-link scrollto"
                onclick="logout(event)"
                ><i class="bi bi-box-arrow-right"></i> <span>Logout</span></a>
            </li>

          </ul>
        </nav>
      </div>
      <form
        id="logoutForm"
        action="/users/logout"
        method="post"
        style="display: none"
      ></form>
    </header>
    <!-- End Header -->

    <!-- ======= Hero Section ======= -->
    <section
      id="hero"
      class="d-flex flex-column justify-content-center align-items-center"
    >
      <div class="hero-container" data-aos="fade-in">
        <h1>Hi, <%= fullname %></h1>
        <p>
          <span
            class="typed"
            data-typed-items="Welcome to Clean Hub, We're the Best Cleaning Platform, Our Agents are the Best in the Game, We care for our Clients"
          ></span>
        </p>
      </div>
    </section>
    <!-- End Hero -->

    <main id="main">
      <!-- ======= About Section ======= -->
      <section id="about" class="about">
        <div class="container">
          <div class="section-title">
            <h2>About</h2>
            <p>
              "Welcome to your personal profile, <%= fullname %>! This is where
              your story unfolds. Dive into your achievements, passions, and
              experiences. Every click reveals a new chapter of your journey.
              Explore, connect, and make this digital haven truly yours. Enjoy
              the journey!"
            </p>
          </div>

          <div class="row">
            <div class="col-lg-4" data-aos="fade-right">

              <img src="<%= photo || '../dash/assets/img/default.png' %>" class="img-fluid" alt="profileImage">

              <!-- <% console.log("Photo path:", photo); %> -->

              <form
                action="/users/photo"
                method="post"
                enctype="multipart/form-data"
              >
                <input type="file" name="profileImage" accept="image/*" />
                <button type="submit">Upload Image</button>
              </form>
            </div>
            <div class="col-lg-8 pt-4 pt-lg-0 content" data-aos="fade-left">
              <h3>Customer Profile Information.</h3>

              <div class="row">
                <div class="col-lg-6">
                  <ul>
                    <li>
                      <i class="bi bi-chevron-right"></i>
                      <strong>Fullname:</strong> <span> <%= fullname %> </span>
                    </li>
                    <li>
                      <i class="bi bi-chevron-right"></i>
                      <strong>Gender:</strong> <span> <%= gender %> </span>
                    </li>
                    <li>
                      <i class="bi bi-chevron-right"></i>
                      <strong>Email:</strong> <span> <%= email %> </span>
                    </li>
                    <li>
                      <i class="bi bi-chevron-right"></i>
                      <strong>Phone Number:</strong>
                      <span> <%= phonenumber %> </span>
                    </li>
                  </ul>
                </div>
                <div class="col-lg-6">
                  <ul>
                    <li>
                      <i class="bi bi-chevron-right"></i>
                      <strong>Date of Birth:</strong>
                      <span> <%= dateofbirth %> </span>
                    </li>
                    <li>
                      <i class="bi bi-chevron-right"></i>
                      <strong>Occupation</strong>
                      <span> <%= occupation %> </span>
                    </li>
                    <li>
                      <i class="bi bi-chevron-right"></i>
                      <strong>Address:</strong> <span> <%= address %> </span>
                    </li>

                  </ul>
                </div>
              </div>
              <p>
              </p>
            </div>
          </div>
        </div>
      </section>
      <!-- End About Section -->

      <!-- ======= Facts Section ======= -->
      <section id="facts" class="facts">
        <div class="container">
          <div class="section-title">
            <h2>Facts</h2>
            <p>
              "Clean Hub, your trusted cleaning service app, combines efficiency with eco-friendly practices. Our expert team ensures spotless spaces while prioritizing sustainability. Experience convenience and reliability as we redefine cleanliness. Your satisfaction is our commitment, making Clean Hub the top choice for a sparkling and environmentally conscious home or office."
            </p>
          </div>

          <div class="row no-gutters">
            <div
              class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch"
              data-aos="fade-up"
            >
              <div class="count-box">
                <i class="bi bi-emoji-smile"></i>
                <span
                  data-purecounter-start="0"
                  data-purecounter-end="250"
                  data-purecounter-duration="1"
                  class="purecounter"
                ></span>
                <p><strong>Happy Clients</strong> who use our services </p>
              </div>
            </div>

            <div
              class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch"
              data-aos="fade-up"
              data-aos-delay="100"
            >
              <div class="count-box">
                <i class="bi bi-journal-richtext"></i>
                <span
                  data-purecounter-start="0"
                  data-purecounter-end="1000"
                  data-purecounter-duration="1"
                  class="purecounter"
                ></span>
                <p><strong> Cleaning Services Rendered </strong> to our esteemed clients montly </p>
              </div>
            </div>

            <div
              class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch"
              data-aos="fade-up"
              data-aos-delay="200"
            >
              <div class="count-box">
                <i class="bi bi-headset"></i>
                <span
                  data-purecounter-start="0"
                  data-purecounter-end="5000"
                  data-purecounter-duration="1"
                  class="purecounter"
                ></span>
                <p><strong>Hours Of Support</strong> Available monthly to our clients </p>
              </div>
            </div>

            <div
              class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch"
              data-aos="fade-up"
              data-aos-delay="300"
            >
              <div class="count-box">
                <i class="bi bi-people"></i>
                <span
                  data-purecounter-start="0"
                  data-purecounter-end="80"
                  data-purecounter-duration="1"
                  class="purecounter"
                ></span>
                <p><strong> Cleaning Agents </strong> Always Available that provide the best service to our clients </p>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- End Facts Section -->

      <!-- ======= Hire A Cleaning Agent ======= -->
      <section id="resume" class="resume">
        <div class="container">
          <div class="section-title">
            <h2>Hire A Cleaning Agent</h2>
            <div id="cleanersList"></div>

            <!-- Modal for hiring a cleaner -->
            <div id="hireCleanerModal" class="modal">
              <div class="modal-content">
                <span class="close" onclick="closeModal('hireCleanerModal')"
                  >&times;</span
                >
                <form id="hireCleanerForm">
                  <h3>Hire Cleaner</h3>
                  <label for="name"> Client Name: </label>
                  <input type="text" id="name" name="name" required />

                  <label for="serviceName">Select Service:</label>
                  <select name="serviceName" id="serviceName">
                    <option value="Residential Cleaning">
                      Residential Cleaning
                    </option>
                    <option value="Commercial Cleaning">
                      Commercial Cleaning
                    </option>
                    <option value="Specialized Cleaning">
                      Specialized Cleaning
                    </option>
                    <option value="Move-In/Move-Out Cleaning">
                      Move-In/Move-Out Cleaning
                    </option>
                    <option value="Post-Construction Cleaning">
                      Post-Construction Cleaning
                    </option>
                    <option value="Real Estate Cleaning">
                      Real Estate Cleaning
                    </option>
                    <option value="Educational Institute Cleaning">
                      Educational Institute Cleaning
                    </option>
                  </select>

                  <label for="description">Description:</label>
                  <textarea
                    id="description"
                    name="description"
                    required
                  ></textarea>

                  <label for="servicePrice">Service Price:</label>
                  <input
                    type="text"
                    id="servicePrice"
                    name="servicePrice"
                    required
                  />

                  <label for="address"> Client Address: </label>
                  <input type="text" id="address" name="address" required />

                  <label for="date"> Date/time: </label>
                  <input type="datetime-local" id="date" name="date" required />

                  <input type="hidden" id="cleanerId" name="cleanerId" />

                  <button type="submit">Hire Cleaner</button>
                </form>
              </div>
            </div>
          </div>

        </div>
      </section>
      <!-- End Resume Section -->

      <!-- ======= Portfolio Section ======= -->
